<div class="row">
  <div class="col-md-3">
    <div class="page-title">
      <h2>State and County here and/or client name</h2>
    </div>
    <table class="table table-hover table-condensed">
      <thead>
        <tr>
          <th>#</th>
          <th>File name</th>
        </tr>
      </thead>
      <tbody>
        #foreach($ftpFile in $session.getFTPFiles())
          <tr>
            <th scope="row">$velocityCount</th>
            <td>$ftpFile.getName()</td>
          </tr>
        #end
      </tbody>
    </table>
  </div>
  <div class="col-md-9">
    <h2>Image Preview</h2>
    <label>Select a file (jpg, png, tiff, pdf)</label>
    <br />
    <div>
      <input type="file" />
      <input type="button" value="click">
    </div>
  </div>
</div>
